<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ui.career.ui.CareerPrepUI">

    <ImageView
    android:id="@+id/user_profile2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.044"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.023"
    app:srcCompat="@drawable/user" />

    <TextView
        android:id="@+id/tv_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/andika_new_basic"
        android:text="@string/masukkan_data_diri"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.164" />

    <TextView
    android:id="@+id/career_opt"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
        android:fontFamily="@font/andika_new_basic_bold"
        android:text="Pengaturan Karir"
        android:textSize="26sp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />


    <ImageView
        android:id="@+id/wrapper_career"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/blue_block"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.343" />


    <TextView
        android:id="@+id/tv_personalInformation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/andika_new_basic"
        android:text="@string/masukkan_data_diri"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.164" />

    <Button
        android:id="@+id/career_input"
        android:layout_width="323dp"
        android:layout_height="58dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="51dp"
        android:background="@color/darkBlue"
        android:fontFamily="@font/andika_new_basic"
        android:text="Input Data Diri"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.916" />

    <ImageView
        android:id="@+id/btn_nav"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/btn_nav_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.959" />

    <ImageButton
        android:id="@+id/home_btn"
        android:layout_width="46dp"
        android:layout_height="43dp"
        android:src="@drawable/home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.147"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.946" />

    <ImageButton
        android:id="@+id/search_btn"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:src="@drawable/search"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.946" />

    <ImageButton
        android:id="@+id/btn_user"
        android:layout_width="38dp"
        android:layout_height="41dp"
        android:src="@drawable/user"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.809"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.949" />

    <EditText
        android:id="@+id/edit_programstudi"
        android:layout_width="329dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="text"
        android:text="@string/program_studi"
        android:textColor="@color/black"
        android:fontFamily="@font/andika_new_basic"
        app:layout_constraintBottom_toBottomOf="@+id/wrapper_career"
        app:layout_constraintEnd_toEndOf="@+id/wrapper_career"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="@+id/wrapper_career"
        app:layout_constraintTop_toTopOf="@+id/wrapper_career"
        app:layout_constraintVertical_bias="0.146" />

    <EditText
        android:id="@+id/edit_experience"
        android:layout_width="325dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="text"
        android:text="@string/pengalaman_kerja"
        android:textColor="@color/black"
        android:fontFamily="@font/andika_new_basic"
        app:layout_constraintBottom_toBottomOf="@+id/wrapper_career"
        app:layout_constraintEnd_toEndOf="@+id/wrapper_career"
        app:layout_constraintStart_toStartOf="@+id/wrapper_career"
        app:layout_constraintTop_toTopOf="@+id/wrapper_career"
        app:layout_constraintVertical_bias="0.289" />

    <EditText
        android:id="@+id/edit_field"
        android:layout_width="325dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="text"
        android:text="@string/bidang_yang_disukai"
        android:textColor="@color/black"
        android:fontFamily="@font/andika_new_basic"
        app:layout_constraintBottom_toBottomOf="@+id/wrapper_career"
        app:layout_constraintEnd_toEndOf="@+id/wrapper_career"
        app:layout_constraintStart_toStartOf="@+id/wrapper_career"
        app:layout_constraintTop_toTopOf="@+id/wrapper_career"
        app:layout_constraintVertical_bias="0.445" />

    <EditText
        android:id="@+id/edit_skill"
        android:layout_width="325dp"
        android:layout_height="44dp"
        android:ems="10"
        android:fontFamily="@font/andika_new_basic"
        android:inputType="text"
        android:text="Skill"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/wrapper_career"
        app:layout_constraintEnd_toEndOf="@+id/wrapper_career"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="@+id/wrapper_career"
        app:layout_constraintTop_toTopOf="@+id/wrapper_career"
        app:layout_constraintVertical_bias="0.734" />

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="@string/jenis_kelamin"
        app:hintTextColor="@color/black"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.402"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:startIconDrawable="@drawable/user">

        <AutoCompleteTextView
            android:id="@+id/tv_dropdown"
            android:layout_width="348dp"
            android:layout_height="48dp"
            android:background="@color/lightBlue"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.65"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.647" />

    </com.google.android.material.textfield.TextInputLayout>




</androidx.constraintlayout.widget.ConstraintLayout>